    "axios": "^0.19.1",
    "core-js": "^3.6.3",
    "flush-promises": "^1.0.2",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.29.2",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
